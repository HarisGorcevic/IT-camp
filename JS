# IT-camp
dugme =() =>{
    for(i of document.getElementsByTagName("h1")){
        i.style.backgroundColor = "black"
    }
}

dugme2 =() =>{
    for(z of document.getElementsByTagName("h2")){
        z.style.backgroundColor = "blue"
    }
}

dugme3 =() =>{
    for(y of document.getElementsByTagName("h3")){
        y.style.backgroundColor = "red"
    }
}

